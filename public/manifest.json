{
  "manifest_version": 3,
  "name": "Login Helper (React)",
  "version": "1.0.0",
  "permissions": ["storage", "scripting", "activeTab", "tabs", "webRequest"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Login Helper",
    "default_icon": {
      "16": "./icons/16.png",
      "32": "./icons/16.png",
      "48": "./icons/16.png",
      "128": "./icons/16.png"
    }
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/16.png",
    "48": "./icons/16.png",
    "128": "./icons/16.png"
  }
}
